{
    "name": "skyraptor/laravel-steam-login",
    "description": "Steam Login package for Laravel 5.5+",
    "keywords": [
        "laravel",
        "authentication",
        "auth",
        "login",
        "steam",
        "openid",
        "lumen",
        "php"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Samuel Maddela",
            "email": "git@maddela.org",
            "homepage": "https://www.maddela.org",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-simplexml": "*",
        "xpaw/steamid": "^2.0.0",
        "guzzlehttp/guzzle": "^6.5.5|^7.0.1"
    },
    "autoload": {
        "psr-4": {
            "skyraptor\\LaravelSteamLogin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "skyraptor\\LaravelSteamLogin\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "skyraptor\\LaravelSteamLogin\\SteamLoginServiceProvider"
            ],
            "aliases": {
                "SteamLogin": "skyraptor\\LaravelSteamLogin\\Facades\\SteamLogin"
            }
        }
    },
    "require-dev": {
        "orchestra/testbench": "^3.8",
        "phpunit/phpunit": "^8.1"
    }
}